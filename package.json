{
  "name": "graphql_server_seed",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/index",
    "build": "tsc",
    "start:dev": "ts-node --files  -r tsconfig-paths/register src/index",
    "generate": "yarn plop --plopfile ./templates/plopfile.js",
    "postgenerate": "yarn codegen",
    "codegen": "graphql-codegen --config codegen.yml",
    "lint": "yarn eslint --ext .tsx --ext .ts src/",
    "lint:fix": "yarn eslint --ext .tsx --ext .ts src/ --fix",
    "test": "NODE_ENV=test jest --config jest.config.js --coverage --detectOpenHandles"
  },
  "dependencies": {
    "apollo-server": "^2.16.1",
    "apollo-server-plugin-response-cache": "^0.9.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-tools": "^7.0.5",
    "inquirer-file-tree-selection-prompt": "^1.0.3",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "pg": "^8.3.0",
    "plop": "^2.5.3",
    "spdy": "^4.0.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.21",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.9.1",
    "@graphql-codegen/schema-ast": "^1.9.1",
    "@graphql-codegen/typescript-operations": "^1.18.0",
    "@graphql-codegen/typescript-resolvers": "^1.9.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.2",
    "@types/ioredis": "^4.17.4",
    "@types/jest": "^26.0.8",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.149",
    "@types/morgan": "^1.7.37",
    "@types/node": "^16.0.3",
    "@types/spdy": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "apollo-server-testing": "^2.16.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^27.0.3",
    "prettier": "^2.0.5",
    "ts-jest": "^27.0.2"
  }
}
